    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.26.4",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.5",
    "rollup-plugin-typescript2": "^0.27.2",
    "sass-loader": "^9.0.3",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {},
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "bracketSpacing": true,
    "insertPragma": false,
    "arrowParens": "avoid",
